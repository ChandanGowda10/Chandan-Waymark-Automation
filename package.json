{
  "name": "waymark",
  "version": "1.0.0",
  "description": "Automation Project",
  "scripts": {
    "selenium-setup": "selenium-standalone install --drivers.chrome.version=94.0.4606.61 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com",
    "selenium-start": "selenium-standalone start --drivers.chrome.version=94.0.4606.61 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com",
    "selenium-setup-circleci": "selenium-standalone install --drivers.chrome.version=78.0.3904.70 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com",
    "selenium-start-circleci": "selenium-standalone start --drivers.chrome.version=78.0.3904.70 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com",
    "e2e-tests": "wdio wdio.conf.js --suite scenario1",
    "e2e-suite": "wdio wdio.conf.js --suite scenario1",
    "e2e-tests-watch": "wdio wdio.conf.js --watch",
    "test:e2e": "./e2e-tests.sh",
    "test:e2e:cache": "./e2e-tests.sh -c",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "allure": "allure serve",
    "jsinspect": "jsinspect ./e2eTests/* ./e2eTests/specs/*"
  },
  "author": "Chandan",
  "license": "ISC",
  "dependencies": {
    "@wdio/cli": "^5.12.0",
    "@wdio/mocha-framework": "^7.7.5",
    "wdio-slack-reporter": "^2.0.1",
    "webcam-easy": "^1.0.5"
  },
  "devDependencies": {
    "@applitools/eyes-selenium": "^4.37.0",
    "@wdio/devtools-service": "^6.0.16",
    "@wdio/junit-reporter": "^5.12.3",
    "@wdio/spec-reporter": "^5.12.0",
    "browsermob-proxy-client": "^0.1.0",
    "chai": "^4.1.2",
    "chromedriver": "^81.0.0",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "serve": "^6.4.3",
    "wdio-allure-reporter": "^0.1.3",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-selenium-standalone-service": "0.0.12",
    "webdriverio": "^4.14.4"
  }
}
